# Gigz V2 Development Environment Variables
# Used for local development with Docker databases

# Database URLs (pointing to Docker containers)
DATABASE_URL=postgresql://gigz:password@localhost:5432/gigz_dev
CLICKHOUSE_URL=http://gigz:password@localhost:8123/gigz_analytics
REDIS_URL=redis://:password@localhost:6379
MEILISEARCH_URL=http://localhost:7700
MEILISEARCH_KEY=masterKey123

# JWT Configuration
JWT_SECRET=dev-jwt-secret-key-for-local-development-only

# Development mode
NODE_ENV=development

# Service ports
AUTH_API_PORT=3002
CORE_API_PORT=3003
CONCERT_API_PORT=3004
SEARCH_API_PORT=3005
GATEWAY_PORT=3001

# Apple Sign-In (development - use test values)
APPLE_CLIENT_ID=dev.test.client
APPLE_TEAM_ID=TESTTEAMID
APPLE_KEY_ID=TESTKEYID
APPLE_PRIVATE_KEY=test-key

# Google Sign-In (development)
GOOGLE_CLIENT_ID=test-google-client-id.apps.googleusercontent.com