# Gigz V2 Production Environment Variables
# Copy to .env.prod and fill in actual values

# Database Configuration
POSTGRES_USER=gigz_prod_user
POSTGRES_PASSWORD=secure_postgres_password_here
CLICKHOUSE_USER=gigz_analytics_user
CLICKHOUSE_PASSWORD=secure_clickhouse_password_here
REDIS_PASSWORD=secure_redis_password_here

# Search Configuration
MEILISEARCH_KEY=secure_meilisearch_master_key_here

# JWT Configuration
JWT_SECRET=secure_jwt_secret_minimum_32_characters_long

# Apple Sign-In Configuration
APPLE_CLIENT_ID=com.yourapp.id
APPLE_TEAM_ID=YOUR_TEAM_ID
APPLE_KEY_ID=YOUR_KEY_ID
APPLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----"

# Google Sign-In Configuration
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com

# Push Notification Configuration
PUSH_NOTIFICATION_KEY=your_push_notification_service_key_here

# Optional: External Services
CLOUDFLARE_R2_ACCESS_KEY=your_r2_access_key
CLOUDFLARE_R2_SECRET_KEY=your_r2_secret_key
CLOUDFLARE_R2_BUCKET=gigz-media
CLOUDFLARE_R2_ENDPOINT=your_account_id.r2.cloudflarestorage.com

# Monitoring & Analytics
SENTRY_DSN=your_sentry_dsn_here
ANALYTICS_API_KEY=your_analytics_api_key_here