# Notification Worker Environment Configuration

# Server Configuration
PORT=3006
NODE_ENV=development

# Apple Push Notification Service (APNs) Configuration
# Get these from Apple Developer Account
APNS_KEY_ID=your_apns_key_id_here
APNS_TEAM_ID=your_team_id_here
APNS_PRIVATE_KEY_PATH=/path/to/your/AuthKey_KEYID.p8
APNS_BUNDLE_ID=com.yourapp.bundleid

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/gigz_v2

# Redis Configuration (for queues and caching)
REDIS_URL=redis://localhost:6379

# Optional: Redis Cluster Configuration
# REDIS_CLUSTER_ENABLED=false
# REDIS_CLUSTER_NODES=redis1:6379,redis2:6379,redis3:6379

# Logging Configuration
LOG_LEVEL=info
# LOG_LEVEL options: fatal, error, warn, info, debug, trace

# Rate Limiting Configuration (optional overrides)
# RATE_LIMIT_USER_GLOBAL_MAX=100
# RATE_LIMIT_USER_GLOBAL_WINDOW_MS=3600000
# RATE_LIMIT_GLOBAL_MAX=10000
# RATE_LIMIT_GLOBAL_WINDOW_MS=60000

# APNs Environment (production/development)
# Automatically determined from NODE_ENV, but can be overridden
# APNS_PRODUCTION=false

# Queue Configuration (optional)
# QUEUE_CONCURRENCY=5
# QUEUE_MAX_STALLED_COUNT=1
# QUEUE_MAX_STALLED_INTERVAL=30000

# Health Check Configuration
# HEALTH_CHECK_TIMEOUT=5000

# Badge Configuration
# BADGE_CACHE_TTL_SECONDS=60
# BADGE_MAX_VALUE=99

# Cleanup Configuration
# DEVICE_CLEANUP_INTERVAL_HOURS=24
# DEVICE_CLEANUP_INACTIVE_DAYS=30
# RATE_LIMIT_CLEANUP_INTERVAL_HOURS=6
# RATE_LIMIT_CLEANUP_OLDER_THAN_MS=86400000